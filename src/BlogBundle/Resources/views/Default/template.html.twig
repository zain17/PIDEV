<html>
	<head>
		<title>{% block titre %}Blog{% endblock %}</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<meta name="description" content="" />
		<meta name="keywords" content="" />
		<link rel="stylesheet" href="{{ asset('bundles/blog/template/assets/css/main.css') }}" />
		{% block css %} {% endblock %}
	</head>
	<body class="is-preload">

		<!-- Header -->
			<header id="header">
				<a class="logo" href="{{ path('blog_index') }}">BonPlan</a>
				<nav>
					<a href="#menu">Menu</a>
				</nav>
			</header>

		<!-- Nav -->
			<nav id="menu">
				<ul class="links">
					{% block elementsMenu %}
					<li><a href="index.html">Home</a></li>
					<li><a href="elements.html">Elements</a></li>
					<li><a href="generic.html">Generic</a></li>
					{% endblock %}
				</ul>
			</nav>

		<!-- Banner -->
		<!-- Footer -->
		{% block content %}
		{% endblock %}

		<!-- Scripts -->
			<script src="{{ asset('bundles/blog/template/assets/js/jquery.min.js') }}"></script>
			<script src="{{ asset('bundles/blog/template/assets/js/browser.min.js') }}"></script>
			<script src="{{ asset('bundles/blog/template/assets/js/breakpoints.min.js') }}"></script>
			<script src="{{ asset('bundles/blog/template/assets/js/util.js') }}"></script>
			<script src="{{ asset('bundles/blog/template/assets/js/main.js') }}"></script>
    {% block scripts %}
    {% endblock %}

	</body>
</html>